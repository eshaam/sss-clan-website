<div class="container push-top ">

  <div class="page-header">
    <h1>Members <small>These are the members of the sSs Clan</small></h1>
  </div>

  <% @members.each do |member| %>

  <div class="row">
    <div class="col-sm-12">
      <div class="jumbotron clearfix">

        <div class="col-sm-9">
            <h3><%= member.psn_gamertag %></h3>
        </div>
        <div class="col-sm-3">
          <p><a class="btn btn-primary btn-lg" href="/members/<%= member.psn_gamertag%>" role="button">View Profile</a></p>
        </div>
      </div>
    </div>
  </div>

  <% end %>


</div>
